@page
@model DigitalHealthCheckWeb.Pages.GPPAQ3Model
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@{
    ViewData["Title"] = "Physical activity page 4 of 4";
}
@{
    var antiForgeryToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken;
}

<component type="typeof(Components.Pages.GPPAQ3Page)"
           render-mode="Static"
           param-antiForgery="@antiForgeryToken"
           param-errorList="@Model.ErrorList"
           param-walking="@Model.Model.Walking"
           param-pace="@Model.Model.Pace"
           param-walkingError="@Model.WalkingError"
           param-paceError="@Model.PaceError" />