@page
@model DigitalHealthCheckWeb.Pages.GPPAQ2Model
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@{
    ViewData["Title"] = "Physical activity page 3 of 4";
}
@{
    var antiForgeryToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken;
}

<component type="typeof(Components.Pages.GPPAQ2Page)"
           render-mode="Static"
           param-antiForgery="@antiForgeryToken"
           param-errorList="@Model.ErrorList"
           param-housework="@Model.Model.Housework"
           param-gardening="@Model.Model.Gardening"
           param-gardeningError="@Model.GardeningError"
           param-houseworkError="@Model.HouseworkError" />