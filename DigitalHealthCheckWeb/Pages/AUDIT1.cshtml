@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@{
    ViewData["Title"] = "Alcohol page 3 of 7";
}
@{
    var antiForgeryToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken;
}

<component type="typeof(Components.Pages.AUDIT1Page)"
           render-mode="Static"
           param-antiForgery="@antiForgeryToken"
           param-errorList="@Model.ErrorList"
           param-units="@Model.Model.Units"
           param-unitsError="@Model.UnitsError"
           param-mSASQ="@Model.Model.MSASQ"
           param-sex="@Model.Sex"
           param-mSASQError="@Model.MSASQError" />