@page
@model DigitalHealthCheckWeb.Pages.BloodSugarModel
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@{
    ViewData["Title"] = "Diabetes page 3 of 3";
}
@{
    var antiForgeryToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken;
}

<component type="typeof(Components.Pages.BloodSugarPage)"
           render-mode="Static"
           param-antiForgery="@antiForgeryToken"
           param-errorList="@Model.ErrorList"
           param-knowYourHbA1c="@Model.Model.KnowYourHbA1c"
           param-hbA1c="@Model.Model.HbA1c"
           param-knowYourHbA1cError="@Model.KnowYourHbA1cError"
           param-hbA1cError="@Model.HbA1cError" />