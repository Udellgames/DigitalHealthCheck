@page
@model DigitalHealthCheckWeb.Pages.TermsAndConditionsPageModel
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@{ViewData["Title"] = "Terms and Conditions";}
@{
    var antiForgeryToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken;
}

<component type="typeof(Components.Pages.TermsAndConditionsPage)"
           render-mode="Static"
           param-antiForgery="@antiForgeryToken"
           param-errorList="@Model.ErrorList"
           param-id="@Model.Id" />