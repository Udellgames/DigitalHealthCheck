@page
@model DigitalHealthCheckWeb.Pages.RiskFactors1Model
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@{
    ViewData["Title"] = "Risk factors page 1 of 3";
}
@{
    var antiForgeryToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken;
}

<component type="typeof(Components.Pages.RiskFactors1Page)"
           render-mode="Static"
           param-antiForgery="@antiForgeryToken"
           param-errorList="@Model.ErrorList"
           param-familyHistoryCVD="@Model.Model.FamilyHistoryCVD"
           param-chronicKidneyDisease="@Model.Model.ChronicKidneyDisease"
           param-atrialFibrillation="@Model.Model.AtrialFibrillation"
           param-familyHistoryCVDError="@Model.FamilyHistoryCVDError"
           param-chronicKidneyDiseaseError="@Model.ChronicKidneyDiseaseError"
           param-atrialFibrillationError="@Model.AtrialFibrillationError" />