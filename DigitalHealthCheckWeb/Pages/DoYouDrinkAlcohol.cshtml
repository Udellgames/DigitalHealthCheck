@page
@model DigitalHealthCheckWeb.Pages.DoYouDrinkAlcoholModel
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@{
    ViewData["Title"] = "Alcohol";
}
@{
    var antiForgeryToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken;
}

<component type="typeof(Components.Pages.DoYouDrinkAlcoholPage)"
           render-mode="Static"
           param-antiForgery="@antiForgeryToken"
           param-errorList="@Model.ErrorList"
           param-drinksAlcohol="@Model.DrinksAlcohol"
           param-error="@Model.Error" />