@page
@model DigitalHealthCheckWeb.Pages.PolycysticOvariesAndGestationalDiabetesModel
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@{
    ViewData["Title"] = "Diabetes page 2 of 3";
}
@{
    var antiForgeryToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken;
}

<component type="typeof(Components.Pages.PolycysticOvariesAndGestationalDiabetesPage)"
           render-mode="Static"
           param-antiForgery="@antiForgeryToken"
           param-errorList="@Model.ErrorList"
           param-polycysticOvaries="@Model.Model.PolycysticOvaries"
           param-gestationalDiabetes="@Model.Model.GestationalDiabetes"
           param-polycysticOvariesError="@Model.PolycysticOvariesError"
           param-gestationalDiabetesError="@Model.GestationalDiabetesError" />